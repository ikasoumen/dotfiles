[alias]
    st = status
	ps = push
	pl = pull
	ch = checkout
	br = branch
	cm = commit
	wdf = diff --word-diff
	lg = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	srt = branch --sort=authordate
	fgt = reset HEAD
[core]
	editor = code --wait
